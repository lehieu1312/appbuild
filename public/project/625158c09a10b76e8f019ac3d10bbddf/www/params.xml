<root>
	<file path="config.xml" >
		<!--<field name="APP_PACHKAGE" required="" default ="false" type="word_site" />-->
		<field name="PACKAGE_ID" label="App package" required="true"  type="string" />
		<field name="APP_NAME" label="App name" required="true" type="string" />
		<field name="APP_DESCRIPTION" label="App description" required="true"  type="string" />
		<field name="APP_AUTHOR" label="Author" required="true" type="string" />
		<field name="APP_EMAIL" label="Email app" required="true" type="email" />
	</file>
	<file path="www/settings.js" >
		<field name="ANDROID_PACKAGE" label="Android package" required="true" type="string" />
		<field name="APPLE_ID" label="App ID" required="true" type="string" />
		<field name="WORDPRESS_URL" label="Wordpress url" required="true" type="wordpress-site" />
		<field name="WORDPRESS_PER_PAGE" label="Per page" required="true" type="number" />
		<field name="ONESIGNAL_APP_ID" label="Onesignal ID" required="true" type="onesignal" />
		<field name="ONESIGNAL_PROJECT_NUMBER" label="Onesignal project" required="true" type="number" />
		<field name="RTL_LANGUAGE" label="rtl language" required="true" type="boolean" />
		<field name="OPEN_TARGET_BLANK" label="Open target blank" required="true" type="boolean" />
		<field name="DATE_FORMAT" label="Date format(e.g: MM-dd-YYYY)" required="true" type="date-format" />
		<field name="GOOGLE_ANALYTICS" label="Google analytics" required="true" type="string" />
	</file>
</root>